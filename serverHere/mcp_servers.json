{
  "mcp_servers": {
    "ddgSearch": {
      "command": "uvx",
      "args": [
        "duckduckgo-mcp-server"
      ]
    },
    "windowsCli": {
      "command": "npx",
      "args": [
        "-y",
        "@simonb97/server-win-cli"
      ]
    },
    "windowsIntegration": {
      "command": "uvx",
      "args": [
        "windows-mcp"
      ],
      "env": {
        "CURL_CA_BUNDLE": "",
        "REQUESTS_CA_BUNDLE": "",
        "PYTHONHTTPSVERIFY": "0",
        "POSTHOG_DISABLED": "true"
      }
    }
  }
}